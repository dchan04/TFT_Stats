@model IEnumerable<TFT_Stats.Models.Companion>
@{
    ViewData["Title"] = "Home Page";

}

<style>
    table, th, td {
        border: 1px solid black;
    }

    table {
        width: 50%
    }
</style>

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
    <h2>HTML Image</h2>
    <img src="https://raw.communitydragon.org/latest/plugins/rcp-be-lol-game-data/global/default/assets/loadouts/companions/tooltip_akalidragon_beatmaker_tier2.png" alt="Trulli" width="500" height="333">
</div>

@foreach (var item in Model)
{
    <table>
        <tr>
            <td>
                <p>Count</p>
                @Html.DisplayFor(modelItem => item.SkinID)
            </td>
        </tr>
    </table>
}


